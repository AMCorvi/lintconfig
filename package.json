{
  "name": "@amcorvi/eslint-config",
  "version": "0.0.0-development",
  "description": "Eslint configuration based on NPM's code style-guide. ",
  "main": "index.js",
  "author": "AMCorvi <am@corvi.co>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/amcorvi/lintconfig.git"
  },
  "bin": {
    "AMConfig": "./bin/AMConfig.js"
  },
  "keywords": [
    "NPM Style Guide",
    "check",
    "checker",
    "code",
    "code checker",
    "code linter",
    "code standards",
    "code style",
    "enforce",
    "eslint",
    "eslintconfig",
    "hint",
    "jscs",
    "jshint",
    "lint",
    "policy",
    "quality",
    "simple",
    "standard",
    "standard style",
    "style",
    "style checker",
    "style linter",
    "verify"
  ],
  "peerDependencies": {
    "eslint": ">= 4.18",
    "eslint-plugin-react": ">= 7.7.0"
  },
  "devDependencies": {
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "jest": "22.4.2",
    "semantic-release": "^15.1.3",
    "semantic-release-cli": "3.6.4"
  },
  "dependencies": {
    "chalk": "2.3.2",
    "chalk-animation": "1.4.0",
    "cli-clear": "1.0.4"
  },
  "scripts": {
    "semantic-release": "semantic-release",
    "test": "jest",
    "test:watch": "jest --watch",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
